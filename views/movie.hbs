<div id="moviepage">
  <img src={{this.Poster}}>
  <div class="details">
    <p>
      <a href="/movies">Go back</a>
    </p>

    <h1>{{movie.Title}}</h1>
    <h2>Director: {{movie.Director}}</h2>
    <img src="{{movie.Poster}}">

    <p>Rated: {{movie.Rated}}</p>

    <p>Duration : {{movie.Runtime}}</p>

    <p>Awards : {{movie.Awards}}</p>
    <ul>
      {{#each movie.Rated}}
      <li>{{this}}</li>
      {{/each}}
    </ul>

    <p>{{movie.Plot}}</p>

    {{#each movie.Runtime}}
    {{this}}
    {{/each}}
  </div>
</div>